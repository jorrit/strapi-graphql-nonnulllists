  },
  "dependencies": {
    "tslib": "^2.7.0"
  },
  "devDependencies": {
    "@strapi/types": "^5.1.0",
    "@strapi/typescript-utils": "^5.1.0",
    "typescript": "5.4.4"
  },
  "peerDependencies": {
    "@apollo/server": "4.13.0",
    "graphql": "^16.8.1"
  },
  "author": {
    "name": "Jorrit Schippers"
  },
  "maintainers": [
    {
      "name": "Jorrit Schippers"
    }
  ],
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "scripts": {
